using System;
using System.Globalization;
class Program
{
    static void Main(string[] args)
    {

        //string[] vet = Console.ReadLine().Split(' ');
        //CultureInfo.InvariantCulture

        int n;

        n = int.Parse(Console.ReadLine());

        for (int i = 1; i <= n; i++)
        {
            if(i % 2 != 0)
            {
                Console.WriteLine(i);
            }
        }

    }
}

