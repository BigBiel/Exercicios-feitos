using System;
using System.Globalization;
class Program
{
    static void Main(string[] args)
    {

        //string[] vet = Console.ReadLine().Split(' ');
        //CultureInfo.InvariantCulture

        int n;

        n = int.Parse(Console.ReadLine());

        for (int i = 1; i <= 10; i++)
        {
            int resultado = i * n;
            Console.WriteLine(i + " x " + n + " = " + resultado);
        }

    }
}
